@model List<MertYazilim.Entities.Concrete.Log.Log>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Method</th>
            <th>Path</th>
            <th>Query</th>
            <th>Created Time</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Method</td>
                <td>@item.Path</td>
                <td>@item.Query</td>
                <td>@item.CreatedTime</td>
            </tr>
        }
    </tbody>
</table>
